{
    "name": "stepanenko3/nova-cards",
    "description": "A Laravel Nova info cards.",
    "keywords": [
        "laravel",
        "nova",
        "cards"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "erusev/parsedown": "^1.7",
        "laravel/framework": "^10.0|^11.0",
        "laravel/nova": "^5.0",
        "spatie/ssl-certificate": "^2.0",
        "stepanenko3/laravel-system-resources": "^1.1.1",
        "stepanenko3/laravel-helpers": "^1.3.2"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaCards\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaCards\\NovaCardsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravelsatis.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "tightenco/duster": "^3.0"
    }
}
